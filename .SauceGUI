        local Toggle = Tabs.Misc:AddToggle("MyToggle", { Title = "ShowGuis", Default = false })

    local isToggled = false

    Toggle:OnChanged(function()
        isToggled = not isToggled
        if isToggled then
            ------------------------------
            GTS = {}
            for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui:GetDescendants()) do
                if v:IsA("Frame") and v.Visible == false then
                    v.Visible = true
                    GTS[#GTS+1] = v
                end
            end
            ------------------------------
        else
            ------------------------------
            for i,v in pairs(GTS) do
                v.Visible = false
            end
            -------------------------------
        end
    end)

    Options.MyToggle:SetValue(false)

